@model IEnumerable<ShopConnection.HinhAnh>
<style>
    .hinhsanpham{
        position: relative;
        width: 100%;
        height: 100px;
        border: 1px solid violet;
        margin-top: 10px;
        line-height:100px;
    }
</style>
<ul class="row">
    @foreach (var item in Model)
    {
        <li class="col-lg-3 col-sm-3 col-xs-4">
            <a href="">
                <div class="hinhsanpham">
                    <img style="margin:0; width:100%;" src="~/Public/images/@item.URL" alt="" />
                </div>
            </a>
        </li>
    }
</ul>